{
  "manifest_version": 2,

  "name": "Ca-Ching",
  "description": "Make Google Sheets a little more fun",
  "version": "1.0",
  "icons": {
    "16": "iconBig.png",
    "48": "iconBig.png",
    "128": "iconBig.png"
  },
  "content_scripts": [{
    "matches": ["*://*.google.com/*"],
    "js": ["content.js"]
  }],
  "web_accessible_resources": ["background.js", "cashregister.mp3"],



  "browser_action": {
    "default_icon": "iconLittle.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}
